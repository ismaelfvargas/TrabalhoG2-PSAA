@model IEnumerable<Entities.Models.AlunoVaga>

@{
    ViewData["Title"] = "Lista";
}

<h1>Lista</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeAluno)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeAluno)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AlunoVagaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AlunoVagaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AlunoVagaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
